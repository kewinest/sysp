gather(key = "Subject", value = "Value", -Parameter)
ggplot(data_long, aes(x = Subject, y = Parameter, fill = Value)) +
geom_tile() +
scale_fill_gradient2(low = "orange", mid = "white", high = "purple", midpoint=0) +
theme_minimal() +
labs(title = "Cost Heatmap",
x = "Subjects", y = "Parameters", fill = "Value") +
theme(axis.text.x = element_text(angle = 45, hjust = 1))
